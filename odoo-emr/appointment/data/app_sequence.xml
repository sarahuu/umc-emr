<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="seq_appointment" model="ir.sequence">
        <field name="name">Appointment</field>
        <field name="code">appointment.appointment</field>
        <field name="prefix">APP/%(year)s/</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_appointment_timeslot" model="ir.sequence">
        <field name="name">Appointment Timeslot</field>
        <field name="code">appointment.timeslot</field>
        <field name="prefix">TSL/%(year)s/</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_appointment_available_slot" model="ir.sequence">
        <field name="name">Appointment Available Slot</field>
        <field name="code">appointment.available.slot</field>
        <field name="prefix">GRS/%(year)s/</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_appointment_visit" model="ir.sequence">
        <field name="name">Appointment Visit</field>
        <field name="code">appointment.visit</field>
        <field name="prefix">VIS/%(year)s/</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
    </record>
</odoo>
