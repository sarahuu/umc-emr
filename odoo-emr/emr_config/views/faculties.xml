<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_university_faculties_list" model="ir.ui.view">
        <field name="name">university.faculties.list</field>
        <field name="model">university.faculties</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="code"/>
                <field name="name"/>

            </list>
        </field>
    </record>


    <record id="action_university_faculties" model="ir.actions.act_window">
        <field name="name">Faculties</field>
        <field name="res_model">university.faculties</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create and manage locations for EMR.
            </p>
        </field>
    </record>
</odoo>