<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="seq_prescription_order" model="ir.sequence">
        <field name="name">Prescription Order</field>
        <field name="code">prescription.order</field>
        <field name="prefix">RX/%(year)s/</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_prescription_order_line" model="ir.sequence">
        <field name="name">Prescription Order Line</field>
        <field name="code">prescription.order.line</field>
        <field name="prefix">RXL/%(year)s/</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_prescription_dispense" model="ir.sequence">
        <field name="name">Prescription Dispense</field>
        <field name="code">prescription.dispense</field>
        <field name="prefix">RXD/%(year)s/</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
    </record>
</odoo>
